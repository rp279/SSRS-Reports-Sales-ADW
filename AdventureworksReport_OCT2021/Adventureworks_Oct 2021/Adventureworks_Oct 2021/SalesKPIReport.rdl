  <DataSets>
    <DataSet Name="DsSalesKPI">
      <Query>
        <DataSourceName>DirectConnectDWH</DataSourceName>
        <CommandText>
WITH CTE AS (

Select  
	b.CalendarYear, 
	CAst(b.CalendarYear as varchar) + ' Q' + Cast(b.CalendarQuarter as varchar) as CalendarQuarter, 
	SUM(a.SalesAmount) as SalesAmount
FROM FactInternetSales a 
INNER JOIN DimDate b on a.OrderDateKey = b.DateKey
group by  
	b.CalendarYear, 
	CAst(b.CalendarYear as varchar) + ' Q' + Cast(b.CalendarQuarter as varchar) 
),

CTE2 AS (

SELECT CalendarYear, CalendarQuarter, 
	SalesAmount as CurrentQuarterSales, 
	LAG(SalesAmount, 1,0) OVER (ORDER BY CalendarQuarter) AS PreviousQuarter
FROM CTE
)


SELECT CalendarYear, CalendarQuarter, CurrentQuarterSales, PreviousQuarter, 
CASE WHEN PreviousQuarter = 0 THEN 0 
	ELSE	100 * (CurrentQuarterSales - PreviousQuarter) / PreviousQuarter
	END AS QuarterlyGrowth

FROM CTE2;</CommandText>
      </Query>
      <Fields>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="CalendarQuarter">
          <DataField>CalendarQuarter</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CurrentQuarterSales">
          <DataField>CurrentQuarterSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PreviousQuarter">
          <DataField>PreviousQuarter</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="QuarterlyGrowth">
          <DataField>QuarterlyGrowth</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="GrowthIndicator">
          <Value>=IIf(Fields!QuarterlyGrowth.Value&gt;=20, 1 ,
IIf(Fields!QuarterlyGrowth.Value&lt;20 and Fields!QuarterlyGrowth.Value&gt;0, 0,-1))</Value>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
